extends ../_extends/extends_html5_prototype.pug

block content
  main
    header.cl-header
      h1 레이아웃
    .prototype-menu
      ul
        li
          a(href="/src/html/cl.html") 페이지 리스트
        li
          a(href="/src/html/_prototype/prototype_list.html") 프로토타입
        li.active
          a(href="/src/html/_prototype/prototype_layout.html") 레이아웃
        li
          a(href="/src/html/_prototype/prototype_form.html") 폼 양식
        li
          a(href="/src/html/_prototype/prototype_components_list.html") 컴포넌트
    
    .prototype-container
      section.prototype-section
        p.subsrc # BreakPoints
          i.caption Desktop : media (min-width: 1024px)
          i.caption Tablet  : media (max-width: 1023px)
          i.caption Mobile  : media (max-width: 767px)

      section.prototype-section
        p.subsrc # Header
        include ../_includes/header.pug
      section.prototype-section
        p.subsrc # Footer
        include ../_includes/footer.pug